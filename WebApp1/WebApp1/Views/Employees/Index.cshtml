@model  IEnumerable<WebApp1.Models.Employee>
@{
    ViewBag.Title = "Index";

    // var employee = ViewData["emp"] as WebApp1.Models.Employee;
    //  var employee1 = ViewBag.emp1;
}

<table border="1">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>Salary</th>
    </tr>

    @foreach (var emp in Model)
    {
        <tr>
            <td>@emp.Empid</td>
            <td>@emp.Name</td>
            <td>@emp.Gender</td>
            <td>@emp.Salary</td>
        </tr>

    }
</table>
